{
  "name": "frost",
  "version": "0.7.0",
  "description": "Open source Vercel alternative",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "migrate": "bun scripts/migrate.ts",
    "setup": "bun src/scripts/setup.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "db:gen": "node ./node_modules/.bin/kysely-gen --url ./data/frost.db --dialect sqlite --camel-case --out ./src/lib/db-types.ts",
    "db:gen:zod": "node ./node_modules/.bin/kysely-gen --url ./data/frost.db --dialect sqlite --camel-case --zod --out ./src/lib/db-schemas.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.2",
    "@orpc/client": "^1.13.2",
    "@orpc/openapi": "^1.13.2",
    "@orpc/server": "^1.13.2",
    "@orpc/zod": "^1.13.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@scalar/api-reference-react": "^0.8.20",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.26.1",
    "kysely": "^0.27.4",
    "kysely-bun-worker": "^1.2.1",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.0.9",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "psl": "^1.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "turndown": "^7.2.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.5",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/turndown": "^5.0.6",
    "better-sqlite3": "^12.6.0",
    "kysely-gen": "0.14.1",
    "postcss": "^8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2"
  }
}
